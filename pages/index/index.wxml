<view class="container">
  <swiper indicator-dots indicator-active-color="#EF8501" indicator-color="#fff" autoplay="3000" interval="5000" duration="500">
    <block wx:for="{{imgUrls}}" wx:key="{{item}}" wx:key="{{item.id}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image"/>
      </swiper-item>
    </block>
  </swiper>
