image {
  width: 100%;
}

.container .tabs-find {
  display: flex;
  justify-content: center;
  padding-top: 38rpx;
  padding-bottom: 12rpx;
  border-top: 4rpx solid #f7f7f7;
}

.container .tabs-find .tab-find {
  width: 240rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background-color: #fff;
  font-size: 28rpx;
  border: 2rpx solid #ccc;
  color: #999;
}

.container .tabs-find .tab-find:first-child {
  border-radius: 8rpx 0 0 8rpx;
}

.container .tabs-find .tab-find:last-child {
  border-radius: 0 8rpx 8rpx 0;
}

.container .tabs-find .tab-find-active {
  border: none;
  color: #fff;
  background-color: #EF8501;
}

.container .tabs-type {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 6rpx solid #f7f7f7;
}

.container .tabs-type .tab-type {
  position: relative;
  flex: 1;
  line-height: 88rpx;
  font-size: 28rpx;
  text-align: center;
  color: #999;
}

.container .tabs-type .tab-type-active {
  color: #EF8501;
}

.container .tabs-type .tab-type-active::after {
  content: "";
  position: absolute;
  bottom: -5rpx;
  left: 50%;
  margin-left: -38rpx;
  width: 76rpx;
  height: 6rpx;
  background-color: #EF8501;
}

.container .card .card-title {
  display: flex;
  align-items: center;
  height: 62rpx;
  margin: 0 20rpx;
  padding-left: 20rpx;
  border-bottom: 2rpx solid #ddd;
  font-size: 26rpx;
  color: #444;
}

.container .card .vip-icon {
  width: 40rpx;
  height: 35rpx;
  margin-right: 10rpx;
}

.container .card .shop-icon {
  width: 38rpx;
  height: 36rpx;
  margin-right: 10rpx;
}

.container .card .card-content {
  padding: 20rpx 31rpx;
  display: flex;
  align-items: center;
  border-bottom: 3rpx solid #f7f7f7;
}

.container .card .card-content .card-img {
  width: 220rpx;
  height: 220rpx;
  margin-right: 43rpx;
}

.container .card .card-content .card-img image {
  width: 100%;
  height: 100%;
}

.container .card .card-content .card-info {
  font-size: 26rpx;
  color: #444;
}

.container .card .card-content .card-info .cell {
  line-height: 2.2;
  display: flex;
  align-items: center;
}

.container .card .card-content .card-info .cell-name {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
