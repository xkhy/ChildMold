<view class="container">
      <view class="tabs">
        <view class="tab {{type==2?'tab-active':''}}" bindtap="changeType" data-type="2">关注的童模</view>
        <view class="tab {{type==1?'tab-active':''}}" bindtap="changeType" data-type="1">关注的商家</view>
      </view>
      <view class="model-group">
          <view class="model-single"  wx:for="{{models}}" wx:key="{{item.id}}" data-id="{{item.id}}" bindtap="toModel">
            <image class="model-image" src="{{item.cover}}"></image>
            <view class="model-info">
              <text class="model-name">{{item.name}}</text>
              <view class="model-attention">
                  <image src="../../../assets/icon/heart.png" class="heart"></image>
                  <text>{{item.fans}}</text>  
              </view>
            </view>
          </view>
      </view>
      </view>